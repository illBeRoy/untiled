{
  "name": "untiled",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.html",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest",
    "start": "parcel src/index.html",
    "build:typescript": "tsc",
    "build:target:web": "TARGET=web parcel build src/index.html --out-dir dist",
    "build:target:desktop": "TARGET=desktop parcel build src/index.html --out-dir dist --target node --no-source-maps --public-url ./ && build --tasks mac-x64 . && cp src/assets/files/** bin/untiled-1.0.0-mac-x64",
    "watch:typescript": "tsc -w",
    "watch": "npm run watch:typescript",
    "lint": "tslint --project tsconfig.json",
    "posttest": "npm run lint"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/file-saver": "^2.0.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.12",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "jest": "^24.9.0",
    "node-sass": "^4.12.0",
    "nwjs-builder-phoenix": "^1.15.0",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^1.4.1",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.6.4"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)$": "ts-jest"
    },
    "roots": [
      "src",
      "test"
    ],
    "testEnvironment": "jsdom",
    "testRegex": "^.+\\.spec\\.(ts|tsx|js)?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "buffer": "^5.4.3",
    "classnames": "^2.2.6",
    "file-saver": "^2.0.2",
    "jimp": "^0.8.4",
    "mobx": "^5.14.0",
    "mobx-react": "^6.1.4",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "window": {
    "width": 1024,
    "height": 768
  },
  "build": {
    "output": "./bin",
    "nwVersion": "0.41.3",
    "files": [
      "dist/**/*",
      "attributions.txt"
    ],
    "mac": {
      "icon": "src/assets/icons/map.icns"
    }
  }
}
